<%= render :partial => 'spree/admin/shared/configuration_menu' %>

<h1><%= t('spree_attached_files.new_attached_file') %></h1>
<%= render 'spree/shared/error_messages', :target => @attached_file %>

<%= form_for [:admin, @attached_file] , :html => { :multipart => true } do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p class="form-buttons" data-hook="buttons"><%= button t(:create) %></p>
<% end %>

